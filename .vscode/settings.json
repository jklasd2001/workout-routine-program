{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "**/node_modules": true
    },
    "explorerExclude.backup": {},
    "npm.packageManager": "pnpm",
    "i18n-ally.localesPaths": [
      "src/messages",
    ],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.sourceLanguage": "ko"
  }
  